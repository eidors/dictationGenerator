[{
        "title": "行路难三首（其一）",
        "author": "李白",
        "years": "唐",
        "content": [
            "金樽清酒斗十千，",
            "，玉盘珍羞值万钱。"
        ]
    },
    {
        "title": "行路难三首（其一）",
        "author": "李白",
        "years": "唐",
        "content": [
            "停杯投箸不能食，",
            "，拔剑四顾心茫然。"
        ]
    },
    {
        "title": "行路难三首（其一）",
        "author": "李白",
        "years": "唐",
        "content": [
            "欲渡黄河冰塞川，",
            "，将登太行雪满山。"
        ]
    },
    {
        "title": "行路难三首（其一）",
        "author": "李白",
        "years": "唐",
        "content": [
            "闲来垂钓碧溪上，",
            "，忽复乘舟梦日边。"
        ]
    },
    {
        "title": "行路难三首（其一）",
        "author": "李白",
        "years": "唐",
        "content": [
            "行路难，",
            "，行路难，",
            "，多歧路，",
            "，今安在？"
        ]
    },
    {
        "title": "行路难三首（其一）",
        "author": "李白",
        "years": "唐",
        "content": [
            "长风破浪会有时，",
            "，直挂云帆济沧海。"
        ]
    },
    {
        "title": "酬乐天扬州初逢席上见赠",
        "author": "刘禹锡",
        "years": "唐",
        "content": [
            "巴山楚水凄凉地，",
            "，二十三年弃置身。"
        ]
    },
    {
        "title": "酬乐天扬州初逢席上见赠",
        "author": "刘禹锡",
        "years": "唐",
        "content": [
            "怀旧空吟闻笛赋，",
            "，到乡翻似烂柯人。"
        ]
    },
    {
        "title": "酬乐天扬州初逢席上见赠",
        "author": "刘禹锡",
        "years": "唐",
        "content": [
            "沉舟侧畔千帆过，",
            "，病树前头万木春。"
        ]
    },
    {
        "title": "酬乐天扬州初逢席上见赠",
        "author": "刘禹锡",
        "years": "唐",
        "content": [
            "今日听君歌一曲，",
            "，暂凭杯酒长精神。"
        ]
    },
    {
        "title": "水调歌头",
        "author": " 苏轼",
        "years": "宋",
        "content": [
            "明月几时有？",
            "？把酒问青天。",
            "。不知天上宫阙，",
            "，今夕是何年？"
        ]
    },
    {
        "title": "水调歌头",
        "author": " 苏轼",
        "years": "宋",
        "content": [
            "我欲乘风归去，",
            "，又恐琼楼玉宇，",
            "，高处不胜寒。",
            "。起舞弄清影，",
            "，何似在人间？"
        ]
    },
    {
        "title": "水调歌头",
        "author": " 苏轼",
        "years": "宋",
        "content": [
            "转朱阁，",
            "，低绮户，",
            "，照无眠。",
            "。不应有恨，",
            "，何事长向别时圆？"
        ]
    },
    {
        "title": "水调歌头",
        "author": " 苏轼",
        "years": "宋",
        "content": [
            "人有悲欢离合，",
            "，月有阴晴圆缺，",
            "，此事古难全。",
            "。但愿人长久，",
            "，千里共婵娟。"
        ]
    },
    {
        "title": "月夜忆舍弟",
        "author": "杜甫",
        "years": "唐",
        "content": [
            "戍鼓断人行，",
            "，秋边一雁声。"
        ]
    },
    {
        "title": "月夜忆舍弟",
        "author": "杜甫",
        "years": "唐",
        "content": [
            "露从今夜白，",
            "，月是故乡明。"
        ]
    },
    {
        "title": "月夜忆舍弟",
        "author": "杜甫",
        "years": "唐",
        "content": [
            "有弟皆分散，",
            "，无家问死生。"
        ]
    },
    {
        "title": "月夜忆舍弟",
        "author": "杜甫",
        "years": "唐",
        "content": [
            "寄书长不达，",
            "，况乃未休兵。"
        ]
    },
    {
        "title": "长沙过贾谊宅",
        "author": "刘长卿",
        "years": "唐",
        "content": [
            "三年谪宦此栖迟，",
            "，万古惟留楚客悲。"
        ]
    },
    {
        "title": "长沙过贾谊宅",
        "author": "刘长卿",
        "years": "唐",
        "content": [
            "秋草独寻人去后，",
            "，寒林空见日斜时。"
        ]
    },
    {
        "title": "长沙过贾谊宅",
        "author": "刘长卿",
        "years": "唐",
        "content": [
            "汉文有道恩犹薄，",
            "，湘水无情吊岂知。"
        ]
    },
    {
        "title": "长沙过贾谊宅",
        "author": "刘长卿",
        "years": "唐",
        "content": [
            "寂寂江山摇落处，",
            "，怜君何事到天涯。"
        ]
    },
    {
        "title": "左迁至蓝关示侄孙湘",
        "author": "韩愈",
        "years": "唐",
        "content": [
            "一封朝奏九重天，",
            "，夕贬潮阳路八千。"
        ]
    },
    {
        "title": "左迁至蓝关示侄孙湘",
        "author": "韩愈",
        "years": "唐",
        "content": [
            "欲为圣朝除弊事，",
            "，肯将衰朽惜残年！"
        ]
    },
    {
        "title": "左迁至蓝关示侄孙湘",
        "author": "韩愈",
        "years": "唐",
        "content": [
            "云横秦岭家何在？",
            "？雪拥蓝关马不前。"
        ]
    },
    {
        "title": "左迁至蓝关示侄孙湘",
        "author": "韩愈",
        "years": "唐",
        "content": [
            "知汝远来应有意，",
            "，好收吾骨瘴江边。"
        ]
    },
    {
        "title": "咸阳城东楼",
        "author": "许浑",
        "years": "唐",
        "content": [
            "一上高城万里愁，",
            "，蒹葭杨柳似汀洲。"
        ]
    },
    {
        "title": "咸阳城东楼",
        "author": "许浑",
        "years": "唐",
        "content": [
            "溪云初起日沉阁，",
            "，山雨欲来风满楼。"
        ]
    },
    {
        "title": "咸阳城东楼",
        "author": "许浑",
        "years": "唐",
        "content": [
            "鸟下绿芜秦苑夕，",
            "，蝉鸣黄叶汉宫秋。"
        ]
    },
    {
        "title": "咸阳城东楼",
        "author": "许浑",
        "years": "唐",
        "content": [
            "行人莫问当年事，",
            "，故国东来渭水流。"
        ]
    },
    {
        "title": "无题",
        "author": " 李商隐",
        "years": "唐",
        "content": [
            "相见时难别亦难，",
            "，东风无力百花残。"
        ]
    },
    {
        "title": "无题",
        "author": " 李商隐",
        "years": "唐",
        "content": [
            "春蚕到死丝方尽，",
            "，蜡炬成灰泪始干。"
        ]
    },
    {
        "title": "无题",
        "author": " 李商隐",
        "years": "唐",
        "content": [
            "晓镜但愁云鬓改，",
            "，夜吟应觉月光寒。"
        ]
    },
    {
        "title": "无题",
        "author": " 李商隐",
        "years": "唐",
        "content": [
            "蓬山此去无多路，",
            "，青鸟殷勤为探看。"
        ]
    },
    {
        "title": "行香子·树绕村庄",
        "author": "秦观",
        "years": "北宋",
        "content": [
            "树绕村庄，",
            "，水满陂塘。",
            "。倚东风、豪兴徜徉。"
        ]
    },
    {
        "title": "行香子·树绕村庄",
        "author": "秦观",
        "years": "北宋",
        "content": [
            "小园几许，",
            "，收尽春光。",
            "。有桃花红，",
            "，李花白，",
            "，菜花黄。"
        ]
    },
    {
        "title": "行香子·树绕村庄",
        "author": "秦观",
        "years": "北宋",
        "content": [
            "远远围墙，",
            "，隐隐茅堂。",
            "。飏青旗、流水桥旁。"
        ]
    },
    {
        "title": "行香子·树绕村庄",
        "author": "秦观",
        "years": "北宋",
        "content": [
            "偶然乘兴、步过东冈。",
            "。正莺儿啼，",
            "，燕儿舞，",
            "，蝶儿忙。"
        ]
    },
    {
        "title": "丑奴儿·书博山道中壁",
        "author": "辛弃疾",
        "years": "宋",
        "content": [
            "少年不识愁滋味，",
            "，爱上层楼。"
        ]
    },
    {
        "title": "丑奴儿·书博山道中壁",
        "author": "辛弃疾",
        "years": "宋",
        "content": [
            "爱上层楼。",
            "。为赋新词强说愁。"
        ]
    },
    {
        "title": "丑奴儿·书博山道中壁",
        "author": "辛弃疾",
        "years": "宋",
        "content": [
            "而今识尽愁滋味，",
            "，欲说还休。"
        ]
    },
    {
        "title": "丑奴儿·书博山道中壁",
        "author": "辛弃疾",
        "years": "宋",
        "content": [
            "欲说还休。",
            "。却道天凉好个秋。"
        ]
    }
]