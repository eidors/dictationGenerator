[{
        "title": "黄鹤楼",
        "author": "崔颢",
        "years": "唐",
        "content": [
            "昔人已乘黄鹤去，",
            "，此地空余黄鹤楼。"
        ]
    },
    {
        "title": "黄鹤楼",
        "author": "崔颢",
        "years": "唐",
        "content": [
            "黄鹤一去不复返，",
            "，白云千载空悠悠。"
        ]
    },
    {
        "title": "黄鹤楼",
        "author": "崔颢",
        "years": "唐",
        "content": [
            "晴川历历汉阳树，",
            "，芳草萋萋鹦鹉洲。"
        ]
    },
    {
        "title": "黄鹤楼",
        "author": "崔颢",
        "years": "唐",
        "content": [
            "日暮乡关何处是？",
            "？烟波江上使人愁。"
        ]
    },
    {
        "title": "使至塞上",
        "author": "唐",
        "years": "王维",
        "content": [
            "单车欲问边，",
            "，属国过居延。"
        ]
    },
    {
        "title": "使至塞上",
        "author": "唐",
        "years": "王维",
        "content": [
            "征蓬出汉塞，",
            "，归雁入胡天。"
        ]
    },
    {
        "title": "使至塞上",
        "author": "唐",
        "years": "王维",
        "content": [
            "大漠孤烟直，",
            "，长河落日圆。"
        ]
    },
    {
        "title": "使至塞上",
        "author": "唐",
        "years": "王维",
        "content": [
            "萧关逢候骑，",
            "，都护在燕然。"
        ]
    },
    {
        "title": "渡荆门送别",
        "author": "唐",
        "years": "李白",
        "content": [
            "渡远荆门外，",
            "，来从楚国游。"
        ]
    },
    {
        "title": "渡荆门送别",
        "author": "唐",
        "years": "李白",
        "content": [
            "山随平野尽，",
            "，江入大荒流。"
        ]
    },
    {
        "title": "渡荆门送别",
        "author": "唐",
        "years": "李白",
        "content": [
            "月下飞天镜，",
            "，云生结海楼。"
        ]
    },
    {
        "title": "渡荆门送别",
        "author": "唐",
        "years": "李白",
        "content": [
            "仍怜故乡水，",
            "，万里送行舟。"
        ]
    },
    {
        "title": "钱塘湖春行",
        "author": "白居易",
        "years": "唐",
        "content": [
            "孤山寺北贾亭西，",
            "，水面初平云脚低。"
        ]
    },
    {
        "title": "钱塘湖春行",
        "author": "白居易",
        "years": "唐",
        "content": [
            "几处早莺争暖树，",
            "，谁家新燕啄春泥。"
        ]
    },
    {
        "title": "钱塘湖春行",
        "author": "白居易",
        "years": "唐",
        "content": [
            "乱花渐欲迷人眼，",
            "，浅草才能没马蹄。"
        ]
    },
    {
        "title": "钱塘湖春行",
        "author": "白居易",
        "years": "唐",
        "content": [
            "最爱湖东行不足，",
            "，绿杨阴里白沙堤。"
        ]
    },
    {
        "title": "野望",
        "author": "王绩",
        "years": "唐",
        "content": [
            "东皋薄暮望，",
            "，徙倚欲何依。"
        ]
    },
    {
        "title": "野望",
        "author": "王绩",
        "years": "唐",
        "content": [
            "树树皆秋色，",
            "，山山唯落晖。"
        ]
    },
    {
        "title": "野望",
        "author": "王绩",
        "years": "唐",
        "content": [
            "牧人驱犊返，",
            "，猎马带禽归。"
        ]
    },
    {
        "title": "野望",
        "author": "王绩",
        "years": "唐",
        "content": [
            "相顾无相识，",
            "，长歌怀采薇。"
        ]
    },
    {
        "title": "龟虽寿",
        "author": "曹操",
        "years": "三国 魏",
        "content": [
            "神龟虽寿，",
            "，犹有竟时。"
        ]
    },
    {
        "title": "龟虽寿",
        "author": "曹操",
        "years": "三国 魏",
        "content": [
            "螣蛇乘雾，",
            "，终为土灰。"
        ]
    },
    {
        "title": "龟虽寿",
        "author": "曹操",
        "years": "三国 魏",
        "content": [
            "老骥伏枥，",
            "，志在千里。"
        ]
    },
    {
        "title": "龟虽寿",
        "author": "曹操",
        "years": "三国 魏",
        "content": [
            "烈士暮年，",
            "，壮心不已。"
        ]
    },
    {
        "title": "龟虽寿",
        "author": "曹操",
        "years": "三国 魏",
        "content": [
            "盈缩之期，",
            "，不但在天。"
        ]
    },
    {
        "title": "龟虽寿",
        "author": "曹操",
        "years": "三国 魏",
        "content": [
            "养怡之福，",
            "，可得永年。"
        ]
    },
    {
        "title": "龟虽寿",
        "author": "曹操",
        "years": "三国 魏",
        "content": [
            "幸甚至哉，",
            "，歌以咏志。"
        ]
    },
    {
        "title": "赠从弟（其二）",
        "author": "刘桢",
        "years": "汉",
        "content": [
            "亭亭山上松，",
            "，瑟瑟谷中风。"
        ]
    },
    {
        "title": "赠从弟（其二）",
        "author": "刘桢",
        "years": "汉",
        "content": [
            "风声一何盛，",
            "，松枝一何劲。"
        ]
    },
    {
        "title": "赠从弟（其二）",
        "author": "刘桢",
        "years": "汉",
        "content": [
            "冰霜正惨凄，",
            "，终岁常端正。"
        ]
    },
    {
        "title": "赠从弟（其二）",
        "author": "刘桢",
        "years": "汉",
        "content": [
            "岂不罹凝寒？",
            "？松柏本有性。"
        ]
    },
    {
        "title": "梁甫行",
        "author": "曹植",
        "years": "魏晋",
        "content": [
            "八方各异气，",
            "，千里殊风雨。"
        ]
    },
    {
        "title": "梁甫行",
        "author": "曹植",
        "years": "魏晋",
        "content": [
            "剧哉边海民，",
            "，寄身于草野。"
        ]
    },
    {
        "title": "梁甫行",
        "author": "曹植",
        "years": "魏晋",
        "content": [
            "妻子象禽兽，",
            "，行止依林阻。"
        ]
    },
    {
        "title": "梁甫行",
        "author": "曹植",
        "years": "魏晋",
        "content": [
            "柴门何萧条，",
            "，狐兔翔我宇。"
        ]
    },
    {
        "title": "庭中有奇树",
        "author": "佚名",
        "years": "两汉",
        "content": [
            "庭中有奇树，",
            "，绿叶发华滋。"
        ]
    },
    {
        "title": "庭中有奇树",
        "author": "佚名",
        "years": "两汉",
        "content": [
            "攀条折其荣，",
            "，将以遗所思。"
        ]
    },
    {
        "title": "庭中有奇树",
        "author": "佚名",
        "years": "两汉",
        "content": [
            "馨香盈怀袖，",
            "，路远莫致之。"
        ]
    },
    {
        "title": "庭中有奇树",
        "author": "佚名",
        "years": "两汉",
        "content": [
            "此物何足贵？",
            "？但感别经时。"
        ]
    },
    {
        "title": "三峡",
        "author": "郦道元",
        "years": "南北朝",
        "content": [
            "重岩叠嶂，",
            "，隐天蔽日，",
            "，自非亭午夜分，",
            "，不见曦月。"
        ]
    },
    {
        "title": "三峡",
        "author": "郦道元",
        "years": "南北朝",
        "content": [
            "至于夏水襄陵，",
            "，沿溯阻绝"
        ]
    },
    {
        "title": "三峡",
        "author": "郦道元",
        "years": "南北朝",
        "content": [
            "其间千二百里，",
            "，虽乘奔御风，",
            "，不以疾也"
        ]
    },
    {
        "title": "三峡",
        "author": "郦道元",
        "years": "南北朝",
        "content": [
            "清荣峻茂，",
            "，良多趣味。"
        ]
    },
    {
        "title": "三峡",
        "author": "郦道元",
        "years": "南北朝",
        "content": [
            "每至晴初霜旦，",
            "，林寒涧肃"
        ]
    },
    {
        "title": "三峡",
        "author": "郦道元",
        "years": "南北朝",
        "content": [
            "巴东三峡巫峡长，",
            "，猿鸣三声泪沾裳。"
        ]
    },
    {
        "title": "答谢中书书",
        "author": "陶弘景",
        "years": "南北朝",
        "content": [
            "高峰入云，",
            "，清流见底"
        ]
    },
    {
        "title": "答谢中书书",
        "author": "陶弘景",
        "years": "南北朝",
        "content": [
            "两岸石壁，",
            "，五色交辉"
        ]
    },
    {
        "title": "答谢中书书",
        "author": "陶弘景",
        "years": "南北朝",
        "content": [
            "青林翠竹，",
            "，四时俱备"
        ]
    },
    {
        "title": "答谢中书书",
        "author": "陶弘景",
        "years": "南北朝",
        "content": [
            "晓雾将歇，",
            "，猿鸟乱鸣"
        ]
    },
    {
        "title": "答谢中书书",
        "author": "陶弘景",
        "years": "南北朝",
        "content": [
            "夕日欲颓，",
            "，沉鳞竞跃"
        ]
    },
    {
        "title": "答谢中书书",
        "author": "陶弘景",
        "years": "南北朝",
        "content": [
            "自康乐以来，",
            "，未复有能与其奇者"
        ]
    },
    {
        "title": "记承天寺夜游",
        "author": "苏轼",
        "years": "宋代",
        "content": [
            "庭下如积水空明，",
            "，水中藻、荇交横，",
            "，盖竹柏影也。"
        ]
    },
    {
        "title": "与朱元思书",
        "author": "吴均",
        "years": "南北朝",
        "content": [
            "从流飘荡，",
            "，任意东西"
        ]
    },
    {
        "title": "与朱元思书",
        "author": "吴均",
        "years": "南北朝",
        "content": [
            "水皆缥碧，",
            "，千丈见底。",
            "。鱼细石，",
            "，直视无碍。",
            "。湍甚箭，",
            "，猛浪若奔。"
        ]
    },
    {
        "title": "与朱元思书",
        "author": "吴均",
        "years": "南北朝",
        "content": [
            "夹岸高山，",
            "，皆生寒树，",
            "，负势竞上，",
            "，互相轩邈，",
            "，争高直指，",
            "，千百成峰。"
        ]
    },
    {
        "title": "与朱元思书",
        "author": "吴均",
        "years": "南北朝",
        "content": [
            "蝉则千转不穷，",
            "，猿则百叫无绝。",
            "。飞戾天者，",
            "，望峰息心"
        ]
    },
    {
        "title": "与朱元思书",
        "author": "吴均",
        "years": "南北朝",
        "content": [
            "经纶世务者，",
            "，窥谷忘反"
        ]
    }
]